{
  "$schema": "http://json.schemastore.org/tsconfig",
	"compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "allowJs": false,
    "strictNullChecks": false,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "experimentalDecorators": true
	}
}